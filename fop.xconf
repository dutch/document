<?xml version="1.0" encoding="UTF-8"?>
<fop version="1.0">
  <renderers>
    <renderer mime="application/pdf">
      <pdf-a-mode>PDF/A-1b</pdf-a-mode>
      <fonts>
        <auto-detect/>
        <substitutions>
          <substitution>
            <from font-family="Helvetica-Bold"/>
            <to font-family="Arial"/>
          </substitution>
        </substitutions>
      </fonts>
    </renderer>
  </renderers>
</fop>
